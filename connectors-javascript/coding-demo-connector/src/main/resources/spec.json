{
  "properties": {
    "name": "CodingDemo",
    "icon": "icon/coding.png",
    "doc" : "${doc}",
    "id": "codingdemo"
  },
  "configOptions": {
    "pdkExpansion": [],
    "node": {
      "type": "object",
      "properties": {
      }
    },
    "connection": {
      "type": "object",
      "properties": {
        "team_name": {
          "type": "String",
          "title": "${teamName}",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "apiServerKey": "database_host",
          "x-index": 10,
          "required": true
        },
        "access_token": {
          "type": "String",
          "title": "${token}",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "apiServerKey": "database_host",
          "x-index": 20,
          "required": true
        },
        "project_name": {
          "title": "${projectName}",
          "type": "string",
          "required": true,
          "x-decorator": "FormItem",
          "x-component": "AsyncSelect",
          "x-component-props": {
            "method": "{{loadCommandList}}",
            "params": "{{ {$values: $values, command: \"DescribeUserProjects\"} }}"
          },
          "x-index": 30,
          "x-reactions": [
            {
              "dependencies": ["team_name", "access_token"],
              "fulfill": {
                "state": {
                  "display": "{{$deps[0]&&$deps[1] ? \"visible\":\"hidden\"}}"
                }
              }
            }
          ]
        },
        "hookButton": {
          "type": "void",
          "title": "${hookButton}",
          "x-decorator": "FormItem",
          "x-component-props": {
            "onClick": "{{useAsyncDataSourceByConfig({service: getToken, withoutField: true}, {name:'hookText'}, $values, $form)}}"
          },
          "x-reactions": {
            "dependencies": ["streamReadType"],
            "fulfill": {
              "state": {
                "display": "{{$deps[0] === \"WebHook\" ? \"visible\" : \"hidden\"}}"
              }
            }
          },
          "x-content": "hookButton",
          "x-component": "Button",
          "x-index": 60,
          "x-display": "hidden"
        },
        "hookText": {
          "type": "string",
          "title": " ",
          "x-decorator": "FormItem",
          "x-decorator-props": {
            "colon": false
          },
          "x-reactions": {
            "dependencies": ["streamReadType"],
            "fulfill": {
              "state": {
                "display": "{{$deps[0] === \"WebHook\" ? \"visible\" : \"hidden\"}}"
              }
            }
          },
          "x-display": "hidden",
          "x-component": "Input",
          "x-index": 70,
          "actionOnCopy" : "NEW_WEB_HOOK_URL"
        }
      }
    }
  },
