{
  "properties": {
    "name": "LarkTask",
    "icon": "icon/task.png",
    "doc": "${doc}",
    "id": "lark-task",
    "authentication": "Alpha",
    "tags": ["SaaS"]
  },
  "configOptions": {
    "pdkExpansion": [],
    "node": {
      "type": "object",
      "properties": {
        "sendType": {
          "type": "String",
          "default": "appoint",
          "title": "${node_send_type}",
          "x-decorator": "FormItem",
          "x-component": "Select",
          "enum": [
            {
              "label": "${node_appoint}",
              "value": "appoint"
            },
            {
              "label": "${node_from_js_node}",
              "value": "from_js_node"
            }
          ],
          "x-index": 10,
          "required": true
        },
        "userType": {
          "type": "String",
          "default": "automatic",
          "title": "${node_user_type}",
          "x-decorator": "FormItem",
          "x-component": "Select",
          "enum": [
            {
              "label": "${node_automatic}",
              "value": "automatic"
            },
            {
              "label": "${node_manual}",
              "value": "manual"
            }
          ],
          "x-reactions": [
            {
              "dependencies": [
                "nodeConfig.sendType"
              ],
              "fulfill": {
                "state": {
                  "display": "{{$deps[0] === \"appoint\" ? \"visible\" : \"hidden\"}}"
                },
                "schema": {
                  "required": "{{$deps[0] === \"appoint\" ? true : false}}"
                }
              }
            }
          ],
          "x-index": 11,
          "required": true
        },
        "ownerArray": {
          "title": "${task_owner}",
          "type": "string",
          "x-decorator": "FormItem",
          "x-component": "FieldSelect",
          "x-component-props": {
            "allowCreate": true,
            "multiple": false,
            "filterable": true
          },
          "x-decorator-props": {
            "tooltip": "${phone_and_email_prompts2}"
          },
          "x-reactions": [
            "{{useAsyncDataSourceByConfig({service: loadNodeFieldOptions, withoutField: true}, $values.$inputs[0])}}",
            {
              "dependencies": [
                "nodeConfig.userType",
                "nodeConfig.sendType"
              ],
              "fulfill": {
                "state": {
                  "display": "{{$deps[0] === \"automatic\" && $deps[1] === \"appoint\" ? \"visible\" : \"hidden\"}}"
                },
                "schema": {
                  "required": "{{$deps[0] === \"automatic\" ? true : false}}"
                }
              }
            }
          ],
          "x-index": 20
        },
        "followerArray": {
