{
  "properties": {
    "name": "Databend",
    "icon": "icons/databend.png",
    "id": "databend",
    "doc": "${doc}",
    "authentication": "Beta",
    "tags": ["Database"]
  },
  "configOptions": {
    "capabilities": [
      {
        "id": "dml_insert_policy",
        "alternatives": [
          "update_on_exists",
          "ignore_on_exists"
        ]
      },
      {
        "id": "dml_update_policy",
        "alternatives": [
          "ignore_on_nonexists",
          "insert_on_nonexists"
        ]
      }
    ],
    "connection": {
      "type": "object",
      "properties": {
        "host": {
          "type": "string",
          "title": "${host}",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "apiServerKey": "database_host",
          "x-index": 1,
          "required": true
        },
        "port": {
          "type": "number",
          "title": "${port}",
          "x-decorator": "FormItem",
          "x-component": "InputNumber",
          "apiServerKey": "database_port",
          "x-index": 2,
          "required": true
        },
        "database": {
          "type": "string",
          "title": "${database}",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "apiServerKey": "database_name",
          "x-index": 3,
          "required": true
        },
        "user": {
          "type": "string",
          "title": "${user}",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "apiServerKey": "database_username",
          "x-index": 4,
          "required": true
        },
        "password": {
          "type": "string",
          "title": "${password}",
          "x-decorator": "FormItem",
          "x-component": "Password",
          "apiServerKey": "database_password",
          "x-index": 5
        },
        "extParams": {
          "type": "string",
          "title": "${extParams}",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "apiServerKey": "additionalString",
          "x-index": 6
        }
      }
    }
  },
  "messages": {
    "default": "en_US",
    "en_US": {
      "host": "Host",
      "port": "Port",
      "database": "database",
      "user": "user",
      "password": "password",
      "extParams": "Connection Parameter String",
      "doc": "docs/databend_en_US.md"
    },
    "zh_CN": {
      "host": "地址",
      "port": "端口",
      "database": "数据库",
      "user": "账号",
      "password": "密码",
      "extParams": "连接参数",
      "doc": "docs/databend_en_US.md"
    },
    "zh_TW": {
      "host": "地址",
      "port": "端口",
      "database": "數據庫",
      "user": "賬號",
      "password": "密碼",
      "extParams": "連接參數",
      "doc": "docs/databend_en_US.md"
    }
  },
  "dataTypes": {
    "String": {
      "to": "TapString",
      "byte": "64k",
      "pkEnablement": false
    },
    "Int8[($zerofill)]": {
      "to": "TapNumber",
      "bit": 8,
      "precision": 3,
      "value": [
        -128,
        127
      ]
    },
    "UInt8[($zerofill)]": {
      "to": "TapNumber",
      "bit": 8,
      "precision": 3,
      "value": [
        0,
        255
      ]
    },
    "Int16[($zerofill)]": {
      "to": "TapNumber",
      "bit": 16,
      "precision": 5,
      "value": [
        -32768,
        32767
      ]
    },
    "UInt16[($zerofill)]": {
      "to": "TapNumber",
      "bit": 16,
      "precision": 5,
      "value": [
        0,
        65535
      ]
    },
    "Int32[($zerofill)]": {
      "to": "TapNumber",
      "bit": 32,
      "precision": 10,
      "value": [
        -2147483648,
        2147483647
      ]
    },
    "UInt32[($zerofill)]": {
      "to": "TapNumber",
      "bit": 32,
      "precision": 10,
      "value": [
        0,
        4294967295
      ]
    },
    "Int64[($zerofill)]": {
      "to": "TapNumber",
      "bit": 64,
      "precision": 19,
      "value": [
        -9223372036854775808,
        9223372036854775807
      ]
    },
    "UInt64[($zerofill)]": {
      "to": "TapNumber",
      "bit": 64,
      "precision": 20,
      "value": [
        0,
        18446744073709551615
      ]
    },
    "Float[($precision,$scale)]": {
      "queryOnly": true,
      "to": "TapNumber",
      "name": "float",
      "precision": [
        1,
        30
      ],
      "scale": [
        0,
        30
      ],
      "fixed": false
    },
    "Double": {
      "queryOnly": true,
      "to": "TapNumber",
      "precision": [
        0,
        30
      ],
      "scale": [
        0,
        30
      ],
      "fixed": false
    },
    "Date": {
      "to": "TapDate",
      "range": [
        "1970-01-01",
        "9999-12-31"
      ],
      "pattern": "yyyy-MM-dd"
    },
    "DateTime[($fraction)]": {
      "to": "TapDateTime",
      "range": [
        "1000-01-01 00:00:00",
        "9999-12-31 23:59:59"
      ],
      "pattern": "yyyy-MM-dd HH:mm:ss",
      "fraction": [
        0,
        6
      ],
      "defaultFraction": 0
    },
    "Array($type)": {
      "to": "TapArray",
      "queryOnly": true
    },
    "Json": {
      "to": "TapMap",
      "byte": "4g",
      "queryOnly": true
    },
    "Tuple($type)": {
      "to": "TapArray",
      "queryOnly": true
    }
  }
}
