  "dataTypes": {
    "Textarea": {"to": "TapString","byte": 5000},
    "Text": {"to": "TapString","byte": 255},
    "Number": {"to": "TapNumber","value": [0,999999999]},
    "Percent": {
      "to": "TapNumber",
      "precision": [ 1, 5],
      "value": [0.0,99999.0],
      "scale": [ 0, 2]
    },
    "Decimal": {
      "to": "TapNumber",
      "precision": [ 1, 16],
      "value": [0.0,99999.0],
      "scale": [ 0, 9]
    },
    "Currency": {
      "to": "TapNumber",
      "precision": [ 1, 16],
      "value": [0.0,99999.0],
      "scale": [ 0, 9]
    },
