  "dataTypes": {
    "StringLonger": {
      "to" : "TapString",
      "byte" : "1m"
    },
    "StringNormal": {
      "to" : "TapString",
      "byte" : 2048
    },
    "StringSmaller": {
      "to" : "TapString",
      "byte" : 512
    },
    "StringMinor": {
      "to" : "TapString",
      "byte" : 256
    },
    "StringBit": {
      "to" : "TapString",
      "byte" : 20
    },
    "Integer": {
      "to": "TapNumber",
      "value" : [-2147483648, 2147483647]
    },
    "Boolean": {"to": "TapBoolean"},
    "Map": {"to" : "TapMap"},
    "Long": {
      "to": "TapNumber",
      "value" : [-9223372036854775808, 9223372036854775807]
    },
    "WorkingHours": {
      "to": "TapNumber",
      "value": [ 0.00 , 10000.00],
      "scale": [ 0, 2]
    },
    "Float": {
      "to": "TapNumber",
      "precision": [ 1, 30],
      "value": [ -3.402823466E+38, 3.402823466E+38],
      "scale": [ 0, 30],
      "fixed": false
    },
    "Array": {"to" : "TapArray"},
    "NULL": {
      "to": "TapString"
    }
  }
}
