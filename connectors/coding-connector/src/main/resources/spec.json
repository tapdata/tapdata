  "dataTypes": {
    "String": {"to" : "TapString","byte" : 1024},
    "Integer": {
      "to": "TapNumber",
      "value" : [-2147483648, 2147483647]
    },
    "Boolean": {"to": "TapBoolean"},
    "Map": {"to" : "TapMap"},
    "Long": {
      "to": "TapNumber",
      "value" : [-9223372036854775808, 9223372036854775807]
    },
    "Double": {
      "to": "TapNumber",
      "precision": [ 1, 11],
      "value": [ -1.7976931348623157E+308, 1.7976931348623157E+308],
      "scale": [ 0, 11],
      "fixed": false
    },
    "Float": {
      "to": "TapNumber",
      "precision": [ 1, 30],
      "value": [ -3.402823466E+38, 3.402823466E+38],
      "scale": [ 0, 30],
      "fixed": false
    },
    "Array": {"to" : "TapArray"}
  }
