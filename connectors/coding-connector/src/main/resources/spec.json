{
  "properties": {
    "name": "Coding",
    "icon": "icons/coding.png",
    "doc" : "${doc}",
    "id": "coding"
  },
  "configOptions": {
    "pdkExpansion": [],
    "node": {
      "type": "object",
      "properties": {
        "iterationCodes": {
          "type": "String",
          "title": "${iterationCodes}",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "x-index": 1,
          "required": true
        },
        "issueType": {
          "type": "String",
          "default": "ALL",
          "title": "${issueType}",
          "x-decorator": "FormItem",
          "x-component": "Select",
          "enum": [
            {
              "label": "${allIssueType}",
              "value": "ALL"
            },
            {
              "label": "${defectIssue}",
              "value": "DEFECT"
            },
            {
              "label": "${requirementIssue}",
              "value": "REQUIREMENT"
            },
            {
              "label": "${missionIssue}",
              "value": "MISSION"
            },
            {
              "label": "${epicIssue}",
              "value": "EPIC"
            }
          ],
          "x-index": 2,
          "required": false
        },
        "": {
          "title": "Space List",
          "type": "string",
          "required": true,
          "x-decorator": "FormItem",
          "x-component": "AsyncSelect",
          "x-component-props": {
            "method": "{{loadCommandList}}",
            "params": "{{ {$values: $values, command: \"spaceList\"} }}"
          },
          "x-reactions": [
            {
              "dependencies": ["apiUrl", "apiToken"],
              "fulfill": {
                "state": {
                  "display": "{{$deps[0]&&$deps[1] ? \"visible\":\"hidden\"}}"
                }
              }
            }
          ]
        }
      }
    },
    "connection": {
      "type": "object",
      "properties": {
        "teamName": {
          "type": "String",
          "title": "${teamName}",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "apiServerKey": "database_host",
          "x-index": 1,
          "required": true
        },
       "token": {
         "type": "String",
         "title": "${token}",
         "x-decorator": "FormItem",
         "x-component": "Input",
         "apiServerKey": "database_host",
         "x-index": 2,
         "required": true
       },
        "projectName": {
          "type": "String",
          "title": "${projectName}",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "apiServerKey": "database_host",
          "x-index": 3,
          "required": true
        }
      }
    }
  },
