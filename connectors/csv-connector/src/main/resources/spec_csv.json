{
  "properties": {
    "name": "CSV",
    "icon": "icons/csv.png",
    "doc": "${doc}",
    "id": "csv",
    "authentication": "Beta",
    "tags": ["File"]
  },
  "configOptions": {
    "connection": {
      "type": "object",
      "properties": {
        "protocol": {
          "type": "string",
          "title": "${protocol}",
          "x-decorator": "FormItem",
          "x-component": "Select",
          "x-index": 10,
          "default": "local",
          "enum": [
            {
              "label": "LOCAL",
              "value": "local"
            },
            {
              "label": "FTP",
              "value": "ftp"
            },
            {
              "label": "SFTP",
              "value": "sftp"
            },
            {
              "label": "SMB",
              "value": "smb"
            },
            {
              "label": "S3FS",
              "value": "s3fs"
            },
            {
              "label": "OSS",
              "value": "oss"
            }
          ],
          "x-reactions": [
            {
              "target": "encoding",
              "fulfill": {
                "state": {
                  "visible": "{{$self.value==='ftp' || $self.value==='sftp'}}"
                }
              }
            },
            {
              "target": "*(ftpHost,ftpPort,ftpUsername,ftpPassword,ftpPassiveMode,ftpConnectTimeout,ftpDataTimeout)",
              "fulfill": {
                "state": {
                  "visible": "{{$self.value==='ftp'}}"
                }
              }
            },
            {
              "target": "*(sftpHost,sftpPort,sftpUsername,sftpPassword)",
              "fulfill": {
                "state": {
                  "visible": "{{$self.value==='sftp'}}"
                }
              }
            },
            {
              "target": "*(smbHost,smbUsername,smbPassword,smbShareDir)",
              "fulfill": {
                "state": {
                  "visible": "{{$self.value==='smb'}}"
                }
              }
            },
            {
              "target": "*(region)",
              "fulfill": {
                "state": {
                  "visible": "{{$self.value==='s3fs'}}"
                }
              }
            },
            {
              "target": "*(accessKey,secretKey,endpoint,bucket)",
              "fulfill": {
                "state": {
                  "visible": "{{$self.value==='oss' || $self.value==='s3fs'}}"
                }
              }
            }
          ]
        },
        "encoding": {
          "type": "string",
          "title": "${encoding}",
          "x-decorator": "FormItem",
          "x-component": "Select",
          "x-index": 20,
          "default": "UTF-8",
          "enum": [
            {
              "label": "UTF-8",
              "value": "UTF-8"
            },
            {
              "label": "UTF-16",
              "value": "UTF-16"
            },
            {
              "label": "GBK",
              "value": "GBK"
            },
            {
              "label": "ASCII",
              "value": "ASCII"
            }
          ]
        },
        "ftpHost": {
          "required": true,
          "type": "string",
          "title": "${ftpHost}",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "apiServerKey": "ftpHost",
          "x-index": 30
        },
        "ftpPort": {
          "required": true,
          "type": "string",
          "title": "${ftpPort}",
          "default": 21,
          "x-decorator": "FormItem",
          "x-component": "InputNumber",
          "apiServerKey": "ftpPort",
          "x-index": 40
        },
        "ftpUsername": {
          "type": "string",
          "title": "${ftpUsername}",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "apiServerKey": "ftpUsername",
          "x-index": 50
        },
        "ftpPassword": {
          "type": "string",
          "title": "${ftpPassword}",
          "x-decorator": "FormItem",
          "x-component": "Password",
          "apiServerKey": "ftpPassword",
          "x-index": 60
        },
        "ftpPassiveMode": {
          "type": "boolean",
          "title": "${ftpPassiveMode}",
          "default": true,
          "x-decorator": "FormItem",
          "x-component": "Switch",
          "x-index": 70
        },
        "ftpConnectTimeout": {
          "required": true,
          "type": "string",
          "title": "${ftpConnectTimeout}",
          "default": 60,
          "x-decorator": "FormItem",
          "x-component": "InputNumber",
          "apiServerKey": "ftpConnectTimeout",
          "x-index": 72
        },
        "ftpDataTimeout": {
          "required": true,
          "type": "string",
          "title": "${ftpDataTimeout}",
          "default": 0,
          "x-decorator": "FormItem",
          "x-component": "InputNumber",
          "apiServerKey": "ftpDataTimeout",
          "x-index": 75
        },
        "sftpHost": {
          "required": true,
          "type": "string",
          "title": "${sftpHost}",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "apiServerKey": "sftpHost",
          "x-index": 80
        },
        "sftpPort": {
          "required": true,
          "type": "string",
          "title": "${sftpPort}",
          "default": 22,
          "x-decorator": "FormItem",
          "x-component": "InputNumber",
          "apiServerKey": "sftpPort",
          "x-index": 90
        },
        "sftpUsername": {
          "required": true,
          "type": "string",
          "title": "${sftpUsername}",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "apiServerKey": "sftpUsername",
          "x-index": 100
        },
        "sftpPassword": {
          "required": true,
          "type": "string",
          "title": "${sftpPassword}",
          "x-decorator": "FormItem",
          "x-component": "Password",
          "apiServerKey": "sftpPassword",
          "x-index": 110
        },
        "smbHost": {
          "required": true,
          "type": "string",
          "title": "${smbHost}",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "apiServerKey": "smbHost",
          "x-index": 120
        },
        "smbUsername": {
          "type": "string",
          "title": "${smbUsername}",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "apiServerKey": "smbUsername",
          "x-index": 130
        },
        "smbPassword": {
          "type": "string",
          "title": "${smbPassword}",
          "x-decorator": "FormItem",
          "x-component": "Password",
          "apiServerKey": "smbPassword",
          "x-index": 140
        },
        "smbShareDir": {
          "type": "string",
          "title": "${smbShareDir}",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "apiServerKey": "smbShareDir",
          "x-index": 145
        },
        "accessKey": {
          "required": true,
          "type": "string",
          "title": "${accessKey}",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "apiServerKey": "accessKey",
          "x-index": 150
        },
        "secretKey": {
          "required": true,
          "type": "string",
          "title": "${secretKey}",
          "x-decorator": "FormItem",
          "x-component": "Password",
          "apiServerKey": "secretKey",
          "x-index": 160
        },
        "endpoint": {
          "required": true,
          "type": "string",
          "title": "${endpoint}",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "apiServerKey": "endpoint",
          "x-index": 170
        },
        "region": {
          "type": "string",
          "title": "${region}",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "apiServerKey": "region",
          "x-index": 180
        },
        "bucket": {
          "required": true,
          "type": "string",
          "title": "${bucket}",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "apiServerKey": "bucket",
          "x-index": 190
        },
        "filePathString": {
          "required": true,
          "type": "string",
          "title": "${filePathString}",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "apiServerKey": "filePathString",
          "x-index": 200
        },
        "loadSchema": {
          "type": "boolean",
          "default": false,
          "x-display": "hidden"
        }
      }
    },
    "node": {
      "type": "object",
      "properties": {
        "modelName": {
          "required": true,
          "type": "string",
          "title": "${modelName}",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "apiServerKey": "modelName",
          "x-index": 1
        },
        "includeRegString": {
          "type": "string",
          "title": "${includeRegString}",
          "default": "*.csv",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "apiServerKey": "includeRegString",
          "x-index": 2,
          "x-reactions": [
            {
              "dependencies": ["$outputs"],
              "fulfill": {
                "state": {
                  "display": "{{$deps[0].length > 0 ? \"visible\":\"hidden\"}}"
                }
              }
            }
          ]
        },
        "excludeRegString": {
          "type": "string",
          "title": "${excludeRegString}",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "apiServerKey": "excludeRegString",
          "x-index": 3,
          "x-reactions": [
            {
              "dependencies": ["$outputs"],
              "fulfill": {
                "state": {
                  "display": "{{$deps[0].length > 0 ? \"visible\":\"hidden\"}}"
                }
              }
            }
          ]
        },
        "recursive": {
          "type": "boolean",
          "title": "${recursive}",
          "default": true,
          "x-decorator": "FormItem",
          "x-component": "Switch",
          "x-index": 4,
          "x-reactions": [
            {
              "dependencies": ["$outputs"],
              "fulfill": {
                "state": {
                  "display": "{{$deps[0].length > 0 ? \"visible\":\"hidden\"}}"
                }
              }
            }
          ]
        },
        "fileEncoding": {
          "type": "string",
          "title": "${fileEncoding}",
          "x-decorator": "FormItem",
          "x-component": "Select",
          "x-index": 6,
          "default": "UTF-8",
          "enum": [
            {
              "label": "UTF-8",
              "value": "UTF-8"
            },
            {
              "label": "UTF-16",
              "value": "UTF-16"
            },
            {
              "label": "GBK",
              "value": "GBK"
            },
            {
              "label": "ASCII",
              "value": "ASCII"
            }
          ]
        },
        "offStandard": {
          "type": "boolean",
          "title": "${offStandard}",
          "default": false,
          "x-decorator": "FormItem",
          "x-component": "Switch",
          "x-index": 7,
          "x-reactions": [
            {
              "dependencies": ["$outputs"],
              "fulfill": {
                "state": {
                  "display": "{{$deps[0].length > 0 ? \"visible\":\"hidden\"}}"
                }
              }
            }
          ]
        },
        "lineExpression": {
          "type": "string",
          "required": true,
          "title": "${lineExpression}",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "x-index": 8,
          "x-reactions": [
            {
              "dependencies": ["$outputs", ".offStandard"],
              "fulfill": {
                "state": {
                  "display": "{{($deps[0].length > 0 && $deps[1]) ? \"visible\":\"hidden\"}}"
                }
              }
            }
          ]
        },
        "header": {
          "type": "string",
          "title": "${header}",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "apiServerKey": "header",
          "x-index": 9
        },
        "headerLine": {
          "type": "string",
          "title": "${headerLine}",
          "default": 1,
          "x-decorator": "FormItem",
          "x-component": "InputNumber",
          "x-index": 10
        },
        "dataStartLine": {
          "required": true,
          "type": "string",
          "title": "${dataStartLine}",
          "default": 2,
          "x-decorator": "FormItem",
          "x-component": "InputNumber",
          "apiServerKey": "dataStartLine",
          "x-index": 11,
          "x-reactions": [
            {
              "dependencies": ["$outputs"],
              "fulfill": {
                "state": {
                  "display": "{{$deps[0].length > 0 ? \"visible\":\"hidden\"}}"
                }
              }
            }
          ]
        },
        "justString": {
          "type": "boolean",
          "title": "${justString}",
          "default": false,
          "x-decorator": "FormItem",
          "x-component": "Switch",
          "x-index": 12,
          "x-reactions": [
            {
              "dependencies": ["$outputs"],
              "fulfill": {
                "state": {
                  "display": "{{$deps[0].length > 0 ? \"visible\":\"hidden\"}}"
                }
              }
            }
          ]
        },
        "writeFilePath": {
          "type": "string",
          "title": "${writeFilePath}",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "apiServerKey": "writeFilePath",
          "x-index": 13,
          "required": true,
          "x-reactions": [
            {
              "dependencies": ["$inputs"],
              "fulfill": {
                "state": {
                  "display": "{{$deps[0].length > 0 ? \"visible\":\"hidden\"}}"
                }
              }
            }
          ]
        },
        "fileNameExpression": {
          "type": "string",
          "title": "${fileNameExpression}",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "apiServerKey": "fileNameExpression",
          "x-index": 14,
          "required": true,
          "x-reactions": [
            {
              "dependencies": ["$inputs"],
              "fulfill": {
                "state": {
                  "display": "{{$deps[0].length > 0 ? \"visible\":\"hidden\"}}"
                }
              }
            }
          ]
        },
        "separator": {
          "type": "string",
          "title": "${separator}",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "apiServerKey": "separator",
          "x-index": 15,
          "required": true,
          "default": ",",
          "x-reactions": [
            {
              "dependencies": ["$inputs"],
              "fulfill": {
                "state": {
                  "display": "{{$deps[0].length > 0 ? \"visible\":\"hidden\"}}"
                }
              }
            }
          ]
        },
        "quoteChar": {
          "type": "string",
          "title": "${quoteChar}",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "apiServerKey": "quoteChar",
          "x-index": 16,
          "required": true,
          "default": "\"",
          "x-reactions": [
            {
              "dependencies": ["$inputs"],
              "fulfill": {
                "state": {
                  "display": "{{$deps[0].length > 0 ? \"visible\":\"hidden\"}}"
                }
              }
            }
          ]
        },
        "lineEnd": {
          "type": "string",
          "title": "${lineEnd}",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "apiServerKey": "lineEnd",
          "x-index": 17,
          "required": true,
          "default": "\\n",
          "x-reactions": [
            {
              "dependencies": ["$inputs"],
              "fulfill": {
                "state": {
                  "display": "{{$deps[0].length > 0 ? \"visible\":\"hidden\"}}"
                }
              }
            }
          ]
        }
      }
    }
  },
