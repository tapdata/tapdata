  "dataTypes": {
    "OBJECT": {
      "to": "TapMap"
    },
    "ARRAY": {
      "to": "TapArray"
    },
    "NUMBER": {
      "precision": [
        1,
        1000
      ],
      "scale": [
        0,
        1000
      ],
      "fixed": true,
      "preferPrecision": 20,
      "preferScale": 8,
      "priority": 1,
      "to": "TapNumber"
    },
    "DATETIME": {
      "range": [
        "1000-01-01 00:00:00.000000000",
        "9999-12-31 23:59:59.999999999"
      ],
      "pattern": "yyyy-MM-dd HH:mm:ss.SSSSSSSSS",
      "fraction": [
        0,
        9
      ],
      "defaultFraction": 3,
      "withTimeZone": false,
      "priority": 2,
      "to": "TapDateTime"
    },
    "INTEGER": {
      "bit": 32,
      "priority": 1,
      "value": [
        -2147483648,
        2147483647
      ],
      "to": "TapNumber"
    },
    "BOOLEAN": {
      "to": "TapBoolean"
    },
    "STRING": {
      "byte": 200,
      "priority": 1,
      "defaultByte": 200,
      "preferByte": 200,
      "to": "TapString"
    },
    "TEXT": {
      "to": "TapString"
    }
  }
