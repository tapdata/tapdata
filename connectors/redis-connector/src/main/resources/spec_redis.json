{
  "properties": {
    "name": "Redis",
    "icon": "icons/redis.png",
    "doc": "${doc}",
    "id": "redis",
    "authentication": "Beta",
    "tags": ["Database"]
  },
  "configOptions": {
    "connection": {
      "type": "object",
      "properties": {
        "deploymentMode": {
          "type": "string",
          "title": "${deploymentMode}",
          "default": "standalone",
          "x-decorator": "FormItem",
          "x-component": "Select",
          "x-index": 1,
          "enum": [
            {
              "label": "${standalone}",
              "value": "standalone"
            },
            {
              "label": "${sentinel}",
              "value": "sentinel"
            }
          ],
          "x-reactions": [
            {
              "target": "*(host,port)",
              "fulfill": {
                "state": {
                  "visible": "{{$self.value === 'standalone'}}"
                }
              }
            },
            {
              "target": "*(sentinelName,sentinelAddress)",
              "fulfill": {
                "state": {
                  "visible": "{{$self.value === 'sentinel'}}"
                }
              }
            }
          ]
        },
        "host": {
          "required": true,
          "type": "string",
          "title": "${host}",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "apiServerKey": "database_host",
          "x-index": 2
        },
        "port": {
          "type": "string",
          "title": "${port}",
          "x-decorator": "FormItem",
          "x-component": "InputNumber",
          "apiServerKey": "database_port",
          "x-index": 3,
          "required": true
        },
        "database": {
          "type": "string",
          "title": "${database}",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "apiServerKey": "database_name",
          "x-index": 4,
          "required": true
        },
        "passwordDisplay": {
          "type": "boolean",
          "title": "${passwordDisplay}",
          "x-decorator": "FormItem",
          "x-component": "Switch",
          "x-index": 6,
          "x-reactions": {
            "target": "password",
            "fulfill": {
              "state": {
                "visible": "{{!!$self.value}}"
              }
            }
          }
        },
        "password": {
          "type": "string",
          "title": "${password}",
          "x-decorator": "FormItem",
          "x-component": "Password",
          "apiServerKey": "database_password",
          "x-index": 7
        },
        "sentinelName": {
          "type": "string",
          "title": "${sentinelName}",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "x-index": 8,
          "required": true
        },
        "sentinelAddress": {
          "type": "array",
          "title": "${sentinelAddress}",
          "x-decorator": "FormItem",
          "x-component": "ArrayItems",
          "x-index": 9,
          "items": {
            "type": "object",
            "properties": {
              "space": {
                "type": "void",
                "x-component": "Space",
                "properties": {
                  "host": {
                    "type": "string",
                    "x-decorator": "FormItem",
                    "x-component": "Input",
                    "x-component-props": {
                      "placeholder": "${Address}"
                    },
                    "x-index": 1
                  },
                  "port": {
                    "type": "number",
                    "x-decorator": "FormItem",
                    "x-component": "InputNumber",
                    "x-component-props": {
                      "placeholder": "${sentinelPort}"
                    },
                    "x-index": 2
                  },
                  "remove": {
                    "type": "void",
                    "x-decorator": "FormItem",
                    "x-component": "ArrayItems.Remove"
                  }
                }
              }
            }
          },
          "properties": {
            "add": {
              "type": "void",
              "title": "${prompt}",
              "x-component": "ArrayItems.Addition"
            }
          }
        }
      }
    },
    "node":{
      "properties":{
        "valueType": {
          "type": "string",
          "title": "${valueType}",
          "default": "String",
          "x-decorator": "FormItem",
          "x-component": "Select",
          "x-index": 1,
          "enum": [
            {
              "label": "String",
              "value": "String"
            },
            {
              "label": "List",
              "value": "List"
            },
            {
              "label": "Hash",
              "value": "Hash"
            }
          ]
        },
        "oneKey": {
          "type": "boolean",
          "title": "${oneKey}",
          "x-decorator": "FormItem",
          "x-component": "Switch",
          "x-index": 2,
          "default": false,
          "x-reactions": [
            {
              "dependencies": [".valueType"],
              "fulfill": {
                "state": {
                  "visible": "{{$deps[0] !== 'String'}}"
                }
              }
            }
          ]
        },
        "keyTableName": {
          "type": "string",
          "title": "${keyTableName}",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "x-index": 3,
          "required": true,
          "x-reactions": [
            {
              "dependencies": [".valueType",".oneKey"],
              "fulfill": {
                "state": {
                  "visible": "{{$deps[0] !== 'String' && $deps[1]}}"
                }
              }
            }
          ]
        },
        "listHead": {
          "type": "boolean",
          "title": "${listHead}",
          "x-decorator": "FormItem",
          "x-component": "Switch",
          "x-index": 4,
          "default": true,
          "x-reactions": [
            {
              "dependencies": [".valueType",".oneKey"],
              "fulfill": {
                "state": {
                  "visible": "{{$deps[0] === 'List' && $deps[1]}}"
                }
              }
            }
          ]
        },
        "keyExpression": {
          "type": "string",
          "title": "${keyExpression}",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "x-index": 5,
          "required": true,
          "x-reactions": [
            {
              "dependencies": [".valueType",".oneKey"],
              "fulfill": {
                "state": {
                  "visible": "{{$deps[0] === 'Hash' || !$deps[1]}}"
                }
              }
            }
          ]
        },
        "valueData": {
          "type": "string",
          "title": "${valueData}",
          "default": "Json",
          "x-decorator": "FormItem",
          "x-component": "Select",
          "x-index": 6,
          "enum": [
            {
              "label": "Text",
              "value": "Text"
            },
            {
              "label": "Json",
              "value": "Json"
            }
          ],
          "x-reactions": {
            "dependencies": [".valueType",".oneKey"],
            "fulfill": {
              "run": "{{$self.value = 'Json'}}",
              "schema": {
                "x-component-props.options": "{{$deps[0] === 'Hash' && !$deps[1] ? [$self.dataSource[1]] : $self.dataSource}}"
              }
            }
          }
        },
        "valueJoinString": {
          "type": "string",
          "title": "${valueJoinString}",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "x-index": 7,
          "default": ",",
          "required": true,
          "x-reactions": [
            {
              "dependencies": [".valueData"],
              "fulfill": {
                "state": {
                  "visible": "{{$deps[0] === 'Text'}}"
                }
              }
            }
          ]
        },
        "csvFormat": {
          "type": "boolean",
          "title": "${csvFormat}",
          "x-decorator": "FormItem",
          "x-component": "Switch",
          "x-index": 8,
          "default": true,
          "x-reactions": [
            {
              "dependencies": [".valueData"],
              "fulfill": {
                "state": {
                  "visible": "{{$deps[0] === 'Text'}}"
                }
              }
            }
          ]
        }
      }
    }
  },
