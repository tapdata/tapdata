{
  "properties": {
    "name": "Custom Connection",
    "icon": "icons/custom.png",
    "doc": "${doc}",
    "id": "custom",
    "authentication": "Beta",
    "tags": ["Database"]
  },
  "configOptions": {
    "connection": {
      "type": "object",
      "properties": {
        "collectionName": {
          "type": "string",
          "title": "${collectionName}",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "required": true,
          "x-reactions": {
            "dependencies": [
              "__TAPDATA.connection_type",
              "collectionName"
            ],
            "fulfill": {
              "schema": {
                "x-decorator-props.style.display": "{{$deps[0]!==\"target\" ? null:\"none\"}}",
                "required": "{{$deps[0]!==\"target\" ? true:false}}"
              }
            }
          },
          "x-index": 10
        },
        "uniqueKeys": {
          "type": "string",
          "title": "${uniqueKeys}",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "required": true,
          "x-reactions": {
            "dependencies": [
              "__TAPDATA.connection_type",
              "uniqueKeys"
            ],
            "fulfill": {
              "schema": {
                "x-decorator-props.style.display": "{{$deps[0]!==\"target\" ? null:\"none\"}}",
                "required": "{{$deps[0]!==\"target\" ? true:false}}"
              }
            }
          },
          "x-index": 20
        },
        "syncType": {
          "type": "string",
          "title": "${syncType}",
          "default": "initial_sync",
          "enum": [
            {
              "label": "${custom_type_initial_sync}",
              "value": "initial_sync"
            },
            {
              "label": "${custom_type_cdc}",
              "value": "cdc"
            },
            {
              "label": "${custom_type_initial_sync_cdc}",
              "value": "initial_sync+cdc"
            }
          ],
          "x-decorator": "FormItem",
          "required": true,
          "x-component": "Select",
          "x-reactions": {
            "dependencies": [
              "__TAPDATA.connection_type",
              "syncType"
            ],
            "fulfill": {
              "schema": {
                "x-decorator-props.style.display": "{{$deps[0]!==\"target\" ? null:\"none\"}}",
                "required": "{{$deps[0]!==\"target\" ? true:false}}"
              }
            }
          },
          "x-index": 30
        },
        "jsEngineName": {
          "type": "string",
          "title": "${jsEngineName}",
          "default": "graal.js",
          "enum": [
            {
              "label": "${jsEngineName_graal}",
              "value": "graal.js"
            },
            {
              "label": "${jsEngineName_nashorn}",
              "value": "nashorn"
            }
          ],
          "x-decorator": "FormItem",
          "required": true,
          "x-component": "Select",
          "x-index": 40
        },
        "customBeforeOpr": {
          "type": "boolean",
          "title": "${customBeforeOpr}",
          "x-decorator": "FormItem",
          "x-component": "Switch",
          "required": true,
          "default": false,
          "show": false,
          "x-reactions": [
            {
              "target": "customBeforeScript",
              "fulfill": {
                "state": {
                  "visible": "{{$self.value===true}}"
                }
              }
            }
          ],
          "x-index": 50
        },
        "customBeforeScript": {
          "title": " ",
          "type": "string",
          "x-decorator": "FormItem",
          "x-decorator-props": {
            "gridSpan": 2,
            "colon": false
          },
          "x-component": "JsEditor",
          "x-component-props": {
            "height": 240,
            "before": "function before() {",
            "after": "}",
            "theme": "one_dark"
          },
          "x-index": 60
        },
        "targetScripTitle": {
          "title": "${targetScript}",
          "type": "void",
          "x-decorator": "FormItem",
          "x-component": "Text",
          "x-component-props": {
            "content": "data = [{ // data is an array\nop : \" i \", // i - insert, u - update, d - delete\nfrom : \" \", // source table name\ndata : { }, // master data\n}]"
          },
          "x-reactions": {
            "dependencies": [
              "__TAPDATA.connection_type"
            ],
            "fulfill": {
              "schema": {
                "x-decorator-props.style.display": "{{$deps[0]!==\"source\" ? null:\"none\"}}",
                "required": "{{$deps[0]!==\"source\" ? true:false}}"
              }
            }
          },
          "x-index": 61
        },
        "targetScript": {
          "title": " ",
          "type": "string",
          "x-decorator": "FormItem",
          "x-decorator-props": {
            "gridSpan": 2,
            "colon": false
          },
          "x-component": "JsEditor",
          "x-component-props": {
            "height": 240,
            "before": "function onData(data) {",
            "after": "}",
            "theme": "one_dark"
          },
          "x-reactions": {
            "dependencies": [
              "__TAPDATA.connection_type"
            ],
            "fulfill": {
              "schema": {
                "x-decorator-props.style.display": "{{$deps[0]!==\"source\" ? null:\"none\"}}"
              }
            }
          },
          "x-index": 70
        },
        "cdcScript": {
          "title": "${cdcScript}",
          "type": "string",
          "x-decorator": "FormItem",
          "x-decorator-props": {
            "gridSpan": 2
          },
          "x-component": "JsEditor",
          "x-component-props": {
            "height": 240,
            "before": "function requestData(ctx) {",
            "after": "}",
            "theme": "one_dark"
          },
          "x-reactions": {
            "dependencies": [
              "__TAPDATA.connection_type",
              "syncType"
            ],
            "fulfill": {
              "schema": {
                "x-decorator-props.style.display": "{{$deps[0]!==\"target\" && $deps[1]!==\"initial_sync\" ? null:\"none\"}}",
                "required": "{{$deps[0]!==\"target\" && $deps[1]!==\"initial_sync\" ? true:false}}"
              }
            }
          },
          "x-index": 80
        },
        "historyScript": {
          "title": "${historyScript}",
          "type": "string",
          "x-decorator": "FormItem",
          "x-decorator-props": {
            "gridSpan": 2
          },
          "x-component": "JsEditor",
          "x-component-props": {
            "height": 240,
            "before": "function requestData() {",
            "after": "}",
            "theme": "one_dark"
          },
          "x-reactions": {
            "dependencies": [
              "__TAPDATA.connection_type",
              "syncType"
            ],
            "fulfill": {
              "schema": {
                "x-decorator-props.style.display": "{{$deps[0]!==\"target\" && $deps[1]!==\"cdc\" ? null:\"none\"}}",
                "required": "{{$deps[0]!==\"target\" && $deps[1]!==\"cdc\" ? true:false}}"
              }
            }
          },
          "x-index": 90
        },
        "customAfterOpr": {
          "type": "boolean",
          "title": "${customAfterOpr}",
          "x-decorator": "FormItem",
          "x-component": "Switch",
          "required": true,
          "default": false,
          "show": false,
          "x-reactions": [
            {
              "target": "customAfterScript",
              "fulfill": {
                "state": {
                  "visible": "{{$self.value===true}}"
                }
              }
            }
          ],
          "x-index": 100
        },
        "customAfterScript": {
          "title": " ",
          "type": "string",
          "x-decorator": "FormItem",
          "x-decorator-props": {
            "gridSpan": 2,
            "colon": false
          },
          "x-component": "JsEditor",
          "x-component-props": {
            "height": 240,
            "before": "function after() {",
            "after": "}",
            "theme": "one_dark"
          },
          "x-index": 110
        }
      }
    }
  },
  "messages": {
    "default": "en_US",
    "en_US": {
      "doc": "docs/custom_en_US.md",
      "collectionName": "collection name",
