      "doc": "docs/tidb_zh_CN.md",
      "ticdcUrl": "ticdc 地址",
      "mqUsername": "kafka账号",
      "mqPassword": "kafka密码",
      "nameSrvAddr": "kafka地址",
      "mqTopic": "kafka主题",
