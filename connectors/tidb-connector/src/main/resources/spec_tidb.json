{
  "properties": {
    "name": "Tidb",
    "icon": "icons/tidb.svg",
    "id": "tidb",
    "doc": "${doc}",
    "authentication": "Beta",
    "tags": [
      "Database"
    ]
  },
  "configOptions": {
    "connection": {
      "type": "object",
      "properties": {
        "pdServer": {
          "type": "string",
          "title": "${pdServer}",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "required": true,
          "x-decorator-props": {
            "tooltip": "${pdServerTip}"
          },
          "x-index": 1
        },
        "host": {
          "type": "string",
          "title": "${host}",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "apiServerKey": "database_host",
          "required": true,
          "x-decorator-props": {
            "tooltip": "${hostTip}"
          },
          "x-index": 2
        },
        "port": {
          "type": "string",
          "title": "${port}",
          "default": 4000,
          "x-decorator": "FormItem",
          "x-component": "InputNumber",
          "apiServerKey": "database_port",
          "required": true,
          "x-decorator-props": {
            "tooltip": "${portTip}"
          },
          "x-index": 3
        },
        "database": {
          "type": "string",
          "title": "${database}",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "apiServerKey": "database_name",
          "required": true,
          "x-decorator-props": {
            "tooltip": "${databaseTip}"
          },
          "x-index": 4
        },
        "user": {
          "type": "string",
          "title": "${user}",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "apiServerKey": "database_username",
          "x-index": 5
        },
        "password": {
          "type": "string",
          "title": "${password}",
          "x-decorator": "FormItem",
          "x-component": "Password",
          "apiServerKey": "database_password",
          "x-index": 6
        },
        "OPTIONAL_FIELDS": {
          "type": "void",
          "properties": {
            "extParams": {
              "type": "string",
              "title": "${extParams}",
              "x-decorator": "FormItem",
              "x-component": "Input",
              "apiServerKey": "additionalString",
              "x-decorator-props": {
                "tooltip": "${extParamsTip}"
              },
              "x-index": 7
            },
            "timezone": {
              "type": "string",
              "title": "${timezone}",
              "default": "",
              "x-decorator": "FormItem",
              "x-decorator-props": {
                "tooltip": "${timezoneTip}"
              },
              "x-component": "Select",
              "enum": [
                {
                  "label": "Database Timezone",
                  "value": ""
                },
                {
                  "label": "UTC -11",
                  "value": "-11:00"
                },
                {
                  "label": "UTC -10",
                  "value": "-10:00"
                },
                {
                  "label": "UTC -09",
                  "value": "-09:00"
                },
                {
                  "label": "UTC -08",
                  "value": "-08:00"
                },
                {
                  "label": "UTC -07",
                  "value": "-07:00"
                },
                {
                  "label": "UTC -06",
                  "value": "-06:00"
                },
                {
                  "label": "UTC -05",
                  "value": "-05:00"
                },
                {
                  "label": "UTC -04",
                  "value": "-04:00"
                },
                {
                  "label": "UTC -03",
                  "value": "-03:00"
                },
                {
                  "label": "UTC -02",
                  "value": "-02:00"
                },
                {
                  "label": "UTC -01",
                  "value": "-01:00"
                },
                {
                  "label": "UTC",
                  "value": "+00:00"
                },
                {
                  "label": "UTC +01",
                  "value": "+01:00"
                },
                {
                  "label": "UTC +02",
                  "value": "+02:00"
                },
                {
                  "label": "UTC +03",
                  "value": "+03:00"
                },
                {
                  "label": "UTC +04",
                  "value": "+04:00"
                },
                {
                  "label": "UTC +05",
                  "value": "+05:00"
                },
                {
                  "label": "UTC +06",
                  "value": "+06:00"
                },
                {
                  "label": "UTC +07",
                  "value": "+07:00"
                },
                {
                  "label": "UTC +08",
                  "value": "+08:00"
                },
                {
                  "label": "UTC +09",
                  "value": "+09:00"
                },
                {
                  "label": "UTC +10",
                  "value": "+10:00"
                },
                {
                  "label": "UTC +11",
                  "value": "+11:00"
                },
                {
                  "label": "UTC +12",
                  "value": "+12:00"
                },
                {
                  "label": "UTC +13",
                  "value": "+13:00"
                },
                {
                  "label": "UTC +14",
                  "value": "+14:00"
                }
              ],
              "x-index": 8
            },
            "mqUsername": {
              "type": "string",
              "title": "${mqUsername}",
              "x-decorator": "FormItem",
              "x-component": "Input",
              "apiServerKey": "mq_username",
              "x-index": 15
            },
            "mqPassword": {
              "type": "string",
              "title": "${mqPassword}",
              "x-decorator": "FormItem",
              "x-component": "Password",
              "apiServerKey": "mq_password",
              "x-index": 14
            },
            "mqTopic": {
              "required": true,
              "type": "string",
              "title": "${mqTopic}",
              "x-decorator": "FormItem",
              "x-component": "Input",
              "apiServerKey": "mq_topics",
              "x-index": 11
            },
            "nameSrvAddr": {
              "required": true,
              "type": "string",
              "title": "${nameSrvAddr}",
              "x-decorator": "FormItem",
              "x-component": "Input",
              "apiServerKey": "mq_nameSrvAddr",
              "x-index": 10
            },
            "ticdcUrl": {
              "required": true,
              "type": "string",
              "title": "${ticdcUrl}",
              "x-decorator": "FormItem",
              "x-component": "Input",
              "apiServerKey": "ticdc_url",
              "x-index": 13
            },
            "enableIncrement": {
              "type": "boolean",
              "title": "${enableIncrement}",
              "default": false,
              "x-decorator": "FormItem",
              "x-component": "Switch",
              "apiServerKey": "enableIncrement",
              "x-reactions": [
                {
                  "target": "*(nameSrvAddr,mqTopic,mqUsername,mqPassword,changefeedId,ticdcUrl)",
                  "fulfill": {
                    "state": {
                      "visible": "{{$self.value===true}}"
                    }
                  }
                }
              ],
              "x-index": 9
            }
          }
        }
      }
    }
  },
  "messages": {
